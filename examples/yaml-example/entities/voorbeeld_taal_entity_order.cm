entity:
    id: "Order"
    description: "Order placed by a customer in the Customer table."
    attributes:
      - name: "Id"
        id: "Id"
        datatype: "Integer"
      - name: "OrderDate"
        id: "OrderDate"
        datatype: "Integer"
      - name: "OrderNumber"
        id: "OrderNumber"
        datatype: "Varchar"
      - name: "CustomerId"
        id: "CustomerId"
        datatype: "Integer"
      - name: "TotalAmount"
        id: "TotalAmount"
        datatype: "Float"